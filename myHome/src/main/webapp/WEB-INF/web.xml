<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		 xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
		 xsi:schemaLocation="http://mxlns.jcp.org/xml/ns/javaee http://mxlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
		 id="WebApp_ID" version="3.1">
<error-page>
	<error-code>404</error-code>
	<location>/template.jsp?BODY=error404.jsp</location>
</error-page>
<error-page>
	<error-code>500</error-code>
	<location>/template.jsp?BODY=error500.jsp</location>
</error-page>

<jsp-config>
	<taglib>
		<taglib-uri>http://bartag.taglib5.com</taglib-uri>
		<taglib-location>/WEB-INF/taglib5.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>http://random.taglib4.com</taglib-uri>
		<taglib-location>/WEB-INF/taglib4.tld</taglib-location>
	</taglib>
</jsp-config>
</web-app>